/*______________________________________________

Header Search Box
______________________________________________*/

.grid--full>.grid__item.header-container {
  display: flex;
  align-items: center;
  padding: 15px 0;
}

.header-social-title{display:none}
.header-search-box{
  float:right; 
  margin-left:auto; 
  flex-basis: 50%;
  top: 19px;
  /*   padding-top:18px; */
  position: relative;
}
.header-bar__search-input[type="search"] {
  width:461px!important; 
  height:41px;
  background: rgba(255,255,255,.3);
  padding:11px 14px 10px !important;  
  font-size:14px; 
  line-height:17px; 
  font-family:'open sans', sans-serif; 
  margin-bottom:0;
}
.header-bar__search-input[type="search"]:focus{
  border-right:0;
  outline:none;
}
.supports-fontface .header-search-box .header-bar__search-submit {
  background: transparent !important;
  position: absolute;
  right: 0;
  color: #fff;
  height: 40px !important;
  font-size: 18px;
  width: 45px !important;
  padding: 6px 12px;
}
.header-bar__search-submit{border-radius:0px!important; -webkit-border-radius:0px!important; -moz-border-radius:0px!important;  float:right!important; height:41px!important; width:41px!important; text-align:center!important;background:#6a1a00 url("//cdn.shopify.com/s/files/1/1045/5578/t/1/assets/search-btn.png?1457448906754365864")!important;}
/* .supports-fontface .header-bar__search-submit .icon-search:before{display:none} */
.header-bar .header-social-meida {
  display: table-cell;
  padding:9px 0px; 
  margin: 0;
  width: 33.33%;
  text-align: left;
}

.top-bar-menu .header-social-meida {
  display: flex;
  align-items: center;
}

.header-message {
  width: 33.33%;
  text-align: center;
  display: table-cell;
  font-size: 15px;
  vertical-align: middle;
  color: #fff;
  padding: 9px 0;
}
.header-message .btn {
  padding: 5px 6px 3px;
  font-size: 12px;
  margin-left: 15px;
  position: relative;
  bottom: 2px;
  text-rendering: optimizeLegibility;
}
.header-social-meida ul{padding:0; margin:0; line-height:normal}
.header-bar .header-social-meida .social-icons li {
  width:24px; 
  height:24px; 
  border-radius:50%; 
  -webkit-border-radius:50%; 
  -moz-border-radius:50%; 
  line-height:20px; 
  text-align:center; 
  color:#fff; 
  margin-right:2px;
}
.top-bar-menu .header-social-meida .social-icons li {
  padding-top: 3px;
  padding-left: 0;
  margin-left: 4px;
}
.top-bar-menu .header-social-meida .social-icons li .icon-facebook {
  color: #4B669B;
  color: #fff;
}
.top-bar-menu .header-social-meida .social-icons li .icon-twitter {
  color: #09AEEC;
  color: #fff;
}
.top-bar-menu .header-social-meida .social-icons li .icon-google {
  color: #C10F16;
  color: #fff;
}
.header-social-meida .social-icons li .icon{font-size:14px; padding-left:0;transition: opacity .5s;}
.header-social-meida .social-icons li .icon:hover {opacity: .8;}
.header-social-meida .social-icons li a{color:#fff;}
.header-social-meida .header-social-title{display:block; font-family: 'TrajanProBold'; font-size:17px; line-height:17px; color:#4c2500;}
.site-header{padding: 0 0px 10px;}
.header-bar__search-form{margin:0}
